            "default": "006"
          },
          {
            "in": "formData",
            "name": "Firstname",
            "type": "string",
            "description": "request Firstname",
            "required": true,
            "default": "วิทยา"
          },
          {
            "in": "formData",
            "name": "Lastname",
            "type": "string",
            "description": "request Lastname",
            "required": true,
            "default": "มหานคร"
          },
          {
            "in": "formData",
            "name": "Status",
            "type": "string",
            "description": "Example Status ; Ready , NotReady , Pending",
            "required": true,
            "default": "Ready"
          }
        ],
        "responses": {
          "201": {
            "description": "Created"
          },
          "400": {
            "description": "Bad Request"
          },
          "500": {
            "description": "Internal Server Error"
          }
        }
      }
    },
