        "consumes": [
          "application/x-www-form-urlencoded"
        ],
        "produces": [
          "application/json"
        ],
        "parameters": [
          {
            "in": "formData",
            "name": "RID",
            "type": "string",
            "description": "Example RID ; 001 ,002 ,003",
            "required": true,
            "default": "001"
          },
          {
            "in": "formData",
            "name": "Firstname",
